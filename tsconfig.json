{
  "compilerOptions": {
    "moduleResolution": "node",
    "baseUrl": "src",
    "paths": {
      "@assets/*": ["assets/*"],
      "@scenes/*": ["scenes/*"],
      "@scripts/*": ["scripts/*"],
      "@utilities/*": ["utilities/*"]
    },
    "esModuleInterop": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true
  }
}
